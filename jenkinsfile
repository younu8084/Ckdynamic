{
    agent any
    
    
    
     
    
   stages{
    stage('Build')
        {
            steps{
              sh 'mvn clean install'
                 }
         }
         
         stage('deploy')
         {
         steps{
         deploy adapters: [tomcat8(credentialsId: 'ba028f2c-746d-4156-99fe-3e01ba7bac94', path: '', url: 'http://13.233.139.13:8880/')], contextPath: 'deploy', onFailure: false, war: '**/*.war'
         }
         }
         }
         }
         
